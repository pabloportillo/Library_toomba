security:
  - access-token: []

components:

  parameters:
    codigo:
      name: codigo
      in: path
      required: true
      description: identificador único del recurso en SAP
      schema:
        type: string
    cliente-codigo:
      name: cliente-codigo
      in: query
      required: true
      description: Código de cliente en SAP
      schema:
        type: string
    codigo-barras:
      name: codigo-barras
      in: query
      required: true
      description: Código de barras
      schema:
        type: string        
    codigo-producto:
      name: codigo-producto
      in: query
      required: true
      description: Código de producto
      schema:
        type: string        
    producto-codigo:
      name: producto-codigo
      in: query
      required: true
      description: Código de producto en SAP
      schema:
        type: string
    codigo-primaria:
      name: codigo-primaria
      in: query
      required: true
      description: Código de la variable primaria (codevar_primario_id)
      schema:
        type: string                 

  responses:
    solicitud-incorrecta:
      description: Faltan parámetros requeridos o existe otro problema con la solicitud.
      content:
        application/json:
          schema:
            $ref: '#/components/schemas/400'
    no-autorizado:
      description: El acceso no está activo o no es válido.
      content:
        application/json:
          schema:
            $ref: '#/components/schemas/401'
    acceso-prohibido:
      description: Acceso prohibido. Sin permiso para ejecutar la acción. Por ej. un empleado trata de hacer algo fuera de sus límites (otro usuario) que solo puede hacer un supervisor.
      content:
        application/json:
          schema:
            $ref: '#/components/schemas/403'
    no-encontrado:
      description: Recurso no encontrado.
      content:
        application/json:
          schema:
            $ref: '#/components/schemas/404'
    ok:
      description: Recurso procesado correctamente.
      content:
        application/json:
          schema:
            $ref: '#/components/schemas/recurso-ok'
        

  schemas:
    400:
      title: 400 - Solicitud incorrecta.
      type: object
      properties:
        estado:
          type: integer
          example: 400
        error:
          type: string
          example: 'Solicitud incorrecta.'

    401:
      title: 401 - No autorizado.
      type: object
      properties:
        estado:
          type: integer
          example: 401
        error:
          type: string
          example: 'No autorizado.'

    403:
      title: 403 - Acceso prohibido.
      type: object
      properties:
        estado:
          type: integer
          example: 403
        error:
          type: string
          example: 'Acceso prohibido.'

    404:
      title: 404 - Recurso no encontrado.
      type: object
      properties:
        estado:
          type: integer
          example: 404
        error:
          type: string
          example: 'Recurso no encontrado.'

    recurso-ok:
      title: Recurso procesado.
      description: Recurso procesado.
      type: object
      properties:
        success:
          type: boolean
          example: true

    cliente-respuesta:
      title: Respuesta cliente
      description: Respuesta a solicitud de cliente
      type: object
      properties:
        success:
          type: boolean
          example: true
        data:
          $ref: '#/components/schemas/cliente-objeto'
        
    subcliente-respuesta:
      title: Respuesta subcliente
      description: Respuesta a solicitud de subcliente
      type: object
      properties:
        success:
          type: boolean
          example: true
        data:
          $ref: '#/components/schemas/subcliente-objeto'

    direccion-respuesta:
      title: Respuesta dirección
      description: Respuesta a solicitud de dirección
      type: object
      properties:
        success:
          type: boolean
          example: true
        data:
          $ref: '#/components/schemas/direccion-objeto'

    envase-respuesta:
      title: Respuesta envase
      description: Respuesta a solicitud de envase
      type: object
      properties:
        success:
          type: boolean
          example: true
        data:
          $ref: '#/components/schemas/envase-objeto'

    exclusion-respuesta:
      title: Respuesta exclusion
      description: Respuesta a solicitud de exclusion
      type: object
      properties:
        success:
          type: boolean
          example: true
        data:
          $ref: '#/components/schemas/exclusion-objeto'

    saldoenvase-respuesta:
      title: Respuesta saldo de envase
      description: Respuesta a solicitud de saldo de envase
      type: object
      properties:
        success:
          type: boolean
          example: true
        data:
          $ref: '#/components/schemas/saldoenvase-objeto'

    producto-respuesta:
      title: Respuesta producto
      description: Respuesta a solicitud de producto
      type: object
      properties:
        success:
          type: boolean
          example: true
        data:
          $ref: '#/components/schemas/producto-objeto'

    variable-respuesta:
      title: Respuesta variable
      description: Respuesta a solicitud de variable
      type: object
      properties:
        success:
          type: boolean
          example: true
        data:
          $ref: '#/components/schemas/variable-objeto'

    variablearticulo-respuesta:
      title: Respuesta variable para artículo
      description: Respuesta a solicitud de variable para artículo
      type: object
      properties:
        success:
          type: boolean
          example: true
        data:
          $ref: '#/components/schemas/variablearticulo-objeto'

    tarifa-respuesta:
      title: Respuesta tarifa
      description: Respuesta a solicitud de tarifa
      type: object
      properties:
        success:
          type: boolean
          example: true
        data:
          $ref: '#/components/schemas/tarifa-objeto'

    precio-respuesta:
