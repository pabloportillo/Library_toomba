      description: Respuesta a solicitud de precio
      type: object
      properties:
        success:
          type: boolean
          example: true
        data:
          $ref: '#/components/schemas/precio-objeto'

    cliente-objeto:
      title: Objeto cliente
      description: Objeto de descripción de un cliente
      type: object
      properties:
        codigo: # Código del cliente en SAP
          type: string
          example: "2554"
        empresa: # Nombre comercial de la empresa
          type: string
          example: "Empresa"
        razonSocial: # Razón social de la empresa
          type: string
          example: "Empresa S.L."
        alias: # Alias del cliente
          type: string
          example: "Alias"          
        cif: # CIF 
          type: string
          example: "B999999"
        telefono: # Teléfono fijo
          type: string
          example: "999 888 777"
        telefono2: # Teléfono fijo 2
          type: string
          example: "999 666 555"
        fax: # Fax
          type: string
          example: "999 444 333"                    
        movil: # Teléfono móvil
          type: string
          example: "666 777 888"
        email: # E-mail
          type: string
          example: "mi@email.com"
        web: # URL web
          type: string
          example: "www.web.com"
        personaContacto: # Nombre de persona de contacto
          type: string
          example: "Nombre Apellidos"
        login: # Login
          type: string
          example: "login"          
        clave: # Contraseña
          type: string
          example: "123456789"
        idioma: # Idioma, código corto de 2 dígitos
          type: string
          example: "23"
        viaDepagoCodigo: # Código SAP de la vía de pago
          type: string
          example: "125"
        viaDePagoTexto: # Texto 
          type: string
          example: "Tarjeta"
        condicionesDePagoCodigo: # Código SAP de las condiciones de pago
          type: string
          example: "5897"
        condicionesDePagoTexto: # Texto del campo
          type: string
          example: "Estándar"
        condicionesDePagoTransporteCodigo: # Código SAP 
          type: string
          example: "574"
        condicionesDePagoTransporteTexto: # Texto del campo
          type: string
          example: "Estándar"
        diaPago: # Dia de pago
          type: string
          example: "01 de Noviembre"
        codigoGrupo: # Codigo de grupo
          type: string
          example: "cod_grp"
        nombreGrupo: # Nombre de grupo
          type: string
          example: "nombre grupo"                              
        iban: # Código IBAN de la cuenta
          type: string
          example: "ES1002378348763546"
        bic: # Código SWIFT BIC de la cuenta
          type: string
          example: "BWHFJDF34"
        tcrMinimo: # TCR mínimo 
          type: integer
          example: 5
        plataformasMinimas: # Nº de plataformas mínimas 
          type: integer
          example: 5
        permitirProgramados: # Permitir programaciones (1, 0)
          type: boolean
          example: false
        subagente: # Subagente
          type: string
          example: "subagente"          
        codigoGrupoImpositivo: # Codigo grupo impositivo 
          type: string
          example: "EI"   
        codigoAgente: # Codigo de comercial 
          type: string
          example: "19"    
        nombreAgente: # Nombre del comercial
          type: string
          example: "Olga Medina"
        codigoEmpVenta: # Codigo de empleado venta
          type: string
          example: "33"
        loginAgente: # Login del comercial
          type: string
          example: "comercial"
        claveAgente: # Clave del comercial
          type: string
          example: "c0ntr4s3ñ4"
        idiomaAgente: # Idioma del agente
          type: string
          example: "23"          
        codigoEmprDest: # Código empresa vendedora para la tabla clientes_vendedores
          type: string
          example: "01_HRUIZ"
        codigoCliente: # Código cliente para la tabla clientes_vendedores
          type: string
          example: "C000213"          
        codigoTarifa: # Código tarifa para la tabla clientes_vendedores
          type: string
          example: "3"          
        limiteCredito: # Limite de credito
          type: number
          example: 145.5
        limiteComprometido: # Limite comprometido
          type: number
          example: 120.3
        importeDispuesto: # Importe dispuesto
          type: number
          example: 81.7
        riesgoConcedido: # Clave del comercial
          type: string
          example: "50"      
        riesgoDisponible: # Clave del comercial
          type: string
          example: "60"            
        estadoCliente: # Estado del cliente ('activo','inactivo','bloqueado')
          type: string
          enum:
            - activo
            - inactivo
            - bloqueado
          example: activo                                                     

    subcliente-objeto:
      title: Objeto subcliente
      description: Objeto de descripción de un subcliente
      type: object
      properties:
        codigo: # Código del subcliente en SAP
          type: string
          example: "2554"
        clienteCodigo: # Código del cliente en SAP
          type: string
          example: "456"
        nombre: # Nombre del subcliente
          type: string
          example: Empresa S.L.

    direccion-objeto:
      title: Objeto dirección
      description: Objeto de descripción de una dirección
      type: object
      properties:
        codigo: # Código de la dirección en SAP
          type: string
          example: "2554"
        clienteCodigo: # Código del cliente en SAP
          type: string
          example: "456"
        tipo: # Tipo de la dirección (‘facturacion’, ‘entrega’)
          type: string
          enum:
            - Facturacion
            - Entrega
          example: Entrega
        direccion: # Dirección
          type: string
          example: C/Luz 5
        localidad: # Localidad
          type: string
          example: Fuengirola
        cp: # Código Postal
          type: string
          example: "29630"
        provincia: # Provincia
          type: string
          example: Madrid
        pais: # País
          type: string
          example: España
        zonaVentaVodigo: # Código de la zona de venta
          type: string
          example: "23345"
        zonaVentaTexto: # Nombre de la zona de venta
          type: string
          example: Zona 5 
        zonaTransporteCodigo: # Código de la zona de transporte
          type: string
          example: "2335"
        zonaTransporteTexto: # Nombre de la zona de transporte
          type: string
          example: Zona 3

    envase-objeto:
      title: Objeto envase
      description: Objeto de descripción de un envase
      type: object
      properties:
        codigo: # Código del envase en SAP
          type: string
          example: "2554"
        vendedorCodigo: # Código del vendedor en SAP
          type: string
          example: "456"
        nombre: # Nombre del envase
          type: string
          example: Plataforma X

    exclusion-objeto:
      title: Objeto exclusion
      description: Objeto de descripción de un exclusion
      type: object
      properties:
        codigo: # Código del exclusion en SAP
          type: string
          example: "2545"
        productoCodigo: # Código del vendedor en SAP
          type: string
          example: "PXT0310C22"
        paisCodigo: # Código del vendedor en SAP
          type: string
          example: "ES"      
        comercialCodigo: # Código del vendedor en SAP
          type: string
          example: "20"               

    saldoenvase-objeto:
      title: Objeto saldo de envase
      description: Objeto de descripción de un saldo de envase
      type: object
      properties:
        codigo: # Código del envase en SAP
          type: string
          example: "2554"
        clienteCodigo: # Código del cliente en SAP
          type: string
          example: "456"
        stock: # Saldo de envases
          type: integer
          example: 45

    producto-objeto:
      title: Objeto producto
      description: Objeto de descripción de un producto
      type: object
      properties:
        codigo: # Código del producto en SAP
          type: string
          example: "2554"
        nombreEs: # Primera parte del nombre del producto en español
          type: string
          example: Dipladenia rio grand type
        nombre2Es: # Segunda parte del nombre del producto en español
