  securitySchemes:
    authorization-token:
      type: http
      description: Token de acceso para iniciar sesión (por defecto 1234567890).
      scheme: bearer
      bearerFormat: token que se debe conocer previamente para poder iniciar sesión.

    access-token:
      type: http
      description: Token de acceso para todas las solicitudes al API una vez que hemos iniciado sesión.
      scheme: bearer
      bearerFormat: token (acces_token) obtenido durante el inicio de sesión.